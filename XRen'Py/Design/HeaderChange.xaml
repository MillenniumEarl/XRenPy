<Window x:Class="X_Ren_Py.HeaderChange"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:X_Ren_Py"
        mc:Ignorable="d"
        Title="Header change" Height="70" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" ContentRendered="Window_ContentRendered" SizeToContent="WidthAndHeight">
    <Grid>
        <Label x:Name="label" Content="New name" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="69" Margin="0,7,0,0"/>
        <TextBox x:Name="returnHeader" Margin="69,10,9,9" TextWrapping="Wrap" KeyUp="returnHeader_KeyUp" Width="116" Height="23"/>

    </Grid>
</Window>
